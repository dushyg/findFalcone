{
  "name": "finding-falcon",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run lint && ng build --prod && npm test && npm run surge-create-200html && npm run build:doc",
    "build:doc": "./node_modules/.bin/compodoc -p tsconfig.app.json --theme Readthedocs --disableSourceCode --disableCoverage -d dist/findingFalcon/documentation/compodoc",
    "serve:doc": "npm run build:doc -- -s",
    "http-server": "npx http-server dist/findingFalcon/ -a localhost -p 4200 -o --proxy http://localhost:4200?",
    "start": "npm run build && npm run http-server",
    "start:prod": "ng serve --prod",
    "start:dev": "ng serve --watch",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "preanalyze": "ng build --prod --stats-json",
    "analyze": "webpack-bundle-analyzer dist/findingFalcon/stats-es2015.json",
    "surge-create-200html": "node -e \"require('fs').copyFile('dist/findingFalcon/index.html', 'dist/findingFalcon/200.html', function(err) { if (err) {console.log(err); throw err;} console.log('File successfully renamed!'); })\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/cdk": "~8.1.1",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "bootstrap": "^4.3.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-devkit/build-angular": "0.803.24",
    "@angular/cli": "~8.3.9",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@babel/compat-data": "7.8.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.24",
    "@types/node": "~8.9.4",
    "angular-in-memory-web-api": "^0.9.0",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^25.3.0",
    "jest-preset-angular": "^8.1.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./setupJest.ts"
    ]
  }
}

{
  "name": "finding-falcone",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:local:app-shell && npm run http-server",
    "start:prod": "ng serve --prod",
    "start:dev": "ng serve --watch",
    "build": "npm run lint && npm run build:app-shell && npm test && npm run surge-create-200html && npm run surge-copy-robots && npm run build:doc",
    "build:local:app-shell": "npm run lint && npm run build:app-shell  && npm test && npm run build:doc",
    "build:app-shell": "ng run findingFalcone:app-shell --configuration=production",
    "http-server": "npx http-server dist/findingFalcone/ --proxy http://localhost:4200? -a localhost -p 4200 -c-1 -o -g",
    "serve:doc": "npm run build:doc -- -s",
    "build:doc": "./node_modules/.bin/compodoc -p tsconfig.app.json --theme Readthedocs --disableSourceCode --disableCoverage -d dist/findingFalcone/documentation/compodoc",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "preanalyze": "ng build --prod --stats-json",
    "analyze": "webpack-bundle-analyzer dist/findingFalcone/stats-es2015.json",
    "surge-create-200html": "node -e \"require('fs').copyFile('dist/findingFalcone/index.html', 'dist/findingFalcone/200.html', function(err) { if (err) {console.log(err); throw err;} console.log('File successfully renamed!'); })\"",
    "surge-copy-robots": "node -e \"require('fs').copyFile('./robots.txt', 'dist/findingFalcone/robots.txt', function(err) { if (err) {console.log(err); throw err;} console.log('Robots.txt successfully copied!'); })\"",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/platform-server": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@angular/service-worker": "~9.1.9",
    "bootstrap": "^4.3.1",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.4.1",
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@babel/compat-data": "7.8.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.11.1",
    "angular-in-memory-web-api": "^0.9.0",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^25.3.0",
    "jest-preset-angular": "^8.1.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./setupJest.ts"
    ]
  }
}
